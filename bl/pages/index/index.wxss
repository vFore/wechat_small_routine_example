page {
  background: #336699;
  font-family: '黑体', '宋体';
}

.title {
  color: white;
  font-size: 20px;
  text-align: center;
  line-height: 30px;
  background-color: orange;
  box-shadow: 2px 2px 10px #888888;
  margin-bottom: 10px;
}

.desc {
  color: white;
}

.time {
  height: 160px;
}

.big-circle {
  width: 150px;
  height: 150px;
  border-radius: 50%;
  position: absolute;
}

.right {
  clip: rect(0,auto,auto,75px);
}

.blue {
  background: #99ccff;
}

.blue1 {
  background: #0099ff;
}

.btnGroup {
  display: flex;
  flex-direction: row;
}

.btn {
  color: white;
  background: #99ccff
}

.circle {
  width: 130px;
  height: 130px;
  position: absolute;
  border-radius: 50%;
  margin: 10px 0 0 10px;
  background-color: white;
}

.circle view {
  margin-left: 50px;
  padding-top: 60px;
  color: black;
  font-size: 20px;
}

.second {
  left: 390rpx;
}